{# Administration - Détails Echec Connexion #}
{% extends "_base.twig" %}

{% block style %}{% endblock %}

{% block description %}Détail Echec{% endblock %}

{% block key %}Détail Echec{% endblock %}

{% block title %}{{ ip }}{% endblock %}

{% block main %}
<section class="contenu">
  <h1>Détails Echec(s)- {{ ip }}</h1>

  {% for liste in listes %}
  <div>
    <label>Matricule : {{ liste.matricule_utilise }} / Mdp : {{ liste.mpd_utilise|slice(0, 20) }} / Détails : {{ liste.commentaire }}</label>
  </div>
  {% endfor %}

  <div class="btn">
    <a href="/delete/{{ ip|replace({'.': '-'}) }}">Débloquer</a>
    <p></p>
  </div>
</section>
{% endblock %}

{% block script %}{% endblock %}
